proc1 = () => {
	varr = arr.slice(0,4);
	
	str = `원래배열 : ${arr} <br>`;
	str += `arr.slice(0,4) : ${varr} <br>`
	
	str += `slice 후 배열 : ${arr} <br>`;
	
	document.getElementById('result1').innerHTML = str;
}

