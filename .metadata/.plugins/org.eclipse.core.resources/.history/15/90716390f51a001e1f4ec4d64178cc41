<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style>
table{
  border : 1px solid blue;
  border-spacing : 10px;
}
td{
  width : 50px;
  height : 50px;
  text-align: center;
}
</style>

<script>
window.onload = function(){
	
	code = `<table border="1">`;
	
	for(i=1; i<=9; i++){
		code += `<tr>`;
		for(j=1; j<=9; j++){
			code += `<td>${i*j}</td>`
		}
		code += `</tr>`;
	}
	
	code += `</table>`;
	
	//result1 검색
	document.querySelector('#result1').innerHTML = code;
	
	str = "";
	//객체 선언
	let myCar = { make: "BMW", model: "X5", year: 2013 };
	for(x in myCar){
		str += `myCar[${x}] : ${myCar[x]} <br>;
	}
	document.querySelector('#result2').innerHTML = str;
	
	
}
</script>
</head>
<body>

</body>
</html>